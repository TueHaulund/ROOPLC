class Foo
    int x

    method run(int a)
        skip

    method test(int y)
        y += 2

class Bar inherits Foo
    int b

    method run(int a)
        b += 6
        a += b
        b -= 6
        call test(a)

class Baz inherits Foo
    int c

    method run(int a)
        c += 5
        a -= c
        x -= 5

class Program
    int result
    int b

    method main()
        construct Bar bar
        construct Baz baz
        call go(bar)
        local int x = 2
        uncall go(baz)
        delocal x = 2
        destruct baz
        destruct bar

    method go(Foo f)
        result += 1
        call f::run(result)